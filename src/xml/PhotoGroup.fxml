<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<ScrollPane minHeight="200.0" vbarPolicy="NEVER" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="xml.PhotoGroupController">
   <content>
      <HBox fx:id="rootHBox" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="200.0" minWidth="-Infinity">
         <children>
            <BorderPane fx:id="controlArea" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="200.0" minWidth="-Infinity" style="-fx-border-color: #000000; -fx-border-width: 10px;">
               <top>
                  <Text fx:id="groupTitle" strokeType="OUTSIDE" strokeWidth="0.0" text="Group Name Here" BorderPane.alignment="CENTER">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Text>
               </top>
               <bottom>
                  <ButtonBar fx:id="menu" BorderPane.alignment="CENTER">
                    <buttons>
                      <Button alignment="CENTER" mnemonicParsing="false" text="Button" textAlignment="CENTER" />
                        <Button mnemonicParsing="false" text="Button" />
                        <Button mnemonicParsing="false" text="Button" />
                        <Button mnemonicParsing="false" text="Button" />
                    </buttons>
                  </ButtonBar>
               </bottom>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </BorderPane>
         </children>
      </HBox>
   </content>
</ScrollPane>
